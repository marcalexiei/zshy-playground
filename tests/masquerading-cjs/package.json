{
  "name": "masquerading-cjs",
  "type": "module",
  "version": "1.0.0",
  "zshy": {
    "exports": {
      ".": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "zshy",
    "typecheck": "tsc --noEmit",
    "check-types": "attw --pack ."
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "types": "./dist/index.d.cts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.2"
  }
}
